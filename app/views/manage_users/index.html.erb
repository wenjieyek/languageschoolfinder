<% @page_title="User List" %>

<%= render(:partial=>'layouts/admin_navigation') %>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">
<h5>Users List </h5>




	<%= grid(@manage_users, show_filters: :when_filtered) do |g| 

			
			g.column name: 'Username' , attribute: 'username' do |user|
			user.username
			end

			g.column name: 'Email'  , attribute: 'email' do |user|
			user.email
			end

			g.column name: 'Action' do |user|
			link_to("Show", manage_user_path(user.id))
			end

end -%>


		


	</div>

	
	
</div>
</div>

</div>

</div>













