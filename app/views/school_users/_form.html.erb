<%= error_messages_for(f.object) %>

<div class="row">
<div class="input-field col s12 m6">
    <%=f.label(:name)%>
    <%=f.text_field(:name,:placeholder=>"Write Your Name")%>
</div>
<div class="input-field col s12 m6">
    <%=f.label(:phonenumber)%>
    <%=f.text_field(:phonenumber)%>
</div>
</div>



<div class="row">
<div class="input-field col s12 m6">
    <%=f.label(:password)%>
    <%=f.password_field(:password)%>
</div>
<div class="input-field col s12 m6">
    <%=f.label(:password)%>
    <%=f.password_field(:password)%>
</div>
</div>



<div class="row">
<div class="input-field col s12 m6">
    <%=f.label(:email)%>
    <%=f.text_field(:email)%>
</div>

<div class="input-field col s12 m6">
    <%=f.label(:website)%>
    <%=f.text_field(:website)%>
</div>
</div>



<div class="row">
<div class="input-field col s12">
    <%=f.label(:address)%>
    <%=f.text_area(:address,:area=>'40x10',:class=>"materialize-textarea")%>
<label for="address">Address</label>
</div>
</div>



<div class="row">
<div class="input-field col s12">
    <%=f.label(:description)%>
    <%=f.text_area(:description,:area=>'40x10',:class=>"materialize-textarea")%>
</div>
</div>



<div class="row">
<div class="input-field col s12">
    <%=f.label(:transportation)%>
    <%=f.text_area(:transportation,:area=>'40x10',:class=>"materialize-textarea")%>
</div>
</div>



<div class="row">
<div class="file-field input-field s12" >
<div class="waves-effect waves-light btn grey darken-4">
      <span>Upload School Picture</span>
      <%=f.file_field(:profilepicture)%>
</div>
<div class="file-path-wrapper">
      <input class="file-path validater" type="text" placeholder="Upload School's Picture" disabled>
</div>
</div>










