


<div class="form-card">
<div class="card center-align white " >
<span class="card-title">Results</span>

<div class="center"><span>Search Results 9 of 9 Results</span></div>




<div class="card-content">

<table class="responsive-table">
		<thead>
			<tr>
			<td></td>
			<td>School Name</td>
			<td>Area</td>
			<td>Ratings</td>
			<td>Action</td>
			</tr>
		</thead>

		<tbody>
	
		<% @results.each do |results| %>
			<tr>
			<td><i class="material-icons teal-text text-lighten-2">school</i></td>
			<td><%= results.name %></td>
			<td><%= results.city %>, <%= results.state %></td>
			<td></td>
			<td><%= link_to("Show",public_path(results.id)) %></td>
			</tr>


		<% end %>




		</tbody>

	</table>

	
	 


</div>
</div><!-- card center-align white -->    
</div><!-- form-card -->




