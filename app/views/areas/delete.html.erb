<%@page_title="Delete Area" %>



<%= render(:partial=>'layouts/admin_navigation') %>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">


	<%=form_for(@area, :method => 'delete') do |f| %>
	<h4>Delete <%=f.object.areaname %> </h4>

	<p>Are you sure you want to permanently delete this area?</p>


	
	
	
	

	<div class="center">

		
		<%= f.submit("Delete Area",:class=>"waves-effect waves-light btn red darken-4") %>
		<%=link_to("Back",areas_path,:class=>'bwaves-effect waves-light btn teal lighten-2')%>
	</div>


	</div>

	<% end %>

		



	
	
</div>
</div>

</div>

</div>


















	