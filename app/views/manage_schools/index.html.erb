<% @page_title="School List" %>

<%= render(:partial=>'layouts/admin_navigation') %>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">
<h5>Schools List </h5>




	<table class="responsive-table">
		<thead>
			<tr>
			<td>School Name</td>
			<td>E-mail</td>
			<td>Action</td>
			
			</tr>
		</thead>

		<tbody>
	
		<% @manage_schools.each do |manage_schools| %>
			<tr>
			<td><%= manage_schools.name %></td>
			<td><%= manage_schools.email %></td>
			<td><%= link_to("Show", manage_school_path(manage_schools.id)) %></td>
			
			
	
			
			
			</tr>


		<% end %>


		</tbody>

	</table>


		


	</div>

	
	
</div>
</div>

</div>

</div>













