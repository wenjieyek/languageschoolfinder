<% @page_title="Delete Attachment" %>



<%= render(:partial=>'layouts/school_navigation' )%>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">

<h5>Delete Attachment</h5>


	<%= form_for(@school_attachment, :method => 'delete') do |f| %>

	

	<p>Are you sure you want to permanently delete this attachment?</p>

	<img class="responsive-img" src= <%=f.object.directory %>>

	
	

	<div class="center">

		
		<%= f.submit("Delete Attachment",:class=>"waves-effect waves-light btn red darken-4") %>
		<%=link_to("Back",school_attachments_path,:class=>'bwaves-effect waves-light btn grey darken-4')%>
	</div>




	<% end %>

		


	</div>

	
	
</div>
</div>

</div>

</div>


















