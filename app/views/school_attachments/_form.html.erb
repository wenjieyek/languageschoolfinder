<%= error_messages_for(f.object) %>


<div class="row">
<div class="input-field col s12">
    
    <%= f.select(:attachmenttype,[
                        ["Picture"],
                        ["Video"]
                        
                       ]) %>


    <%=f.label("Attachment's Type")%>                   



</div>
</div>




<div class="row">
<div class="file-field input-field s12" >
<div class="waves-effect waves-light btn teal lighten-2">
      <span>Upload School Attachment</span>
      <%=f.file_field(:directory)%>
</div>
<div class="file-path-wrapper">
      <input class="file-path validater" type="text" placeholder="Upload Course's Attachment" disabled>
</div>
</div>
</div>


<div class="row">
<div class="input-field col s12">
    <%=f.label("Description")%>
    <%=f.text_area(:description,:area=>'40x10',:class=>"materialize-textarea")%>
</div>
</div>













