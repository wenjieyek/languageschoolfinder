<% @page_title="Attachment Details" %>



<div class="form-card"> <!-- margin top 5% -->
<div class="row">

<%= render(:partial=>'layouts/school_navigation') %>

<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
	<div class="card-content">

	<h5>Attachment Details </h5>


			
		<label>Attachment</label>
			
			<p><img class="responsive-img" style="height:400px;" src=<%= @school_attachment.directory %> ></p>

		<label>Attachment Type</label>
			
			<p><%= @school_attachment.attachmenttype %></p>

			<label>Description</label>

			<p><%= @school_attachment.description %></p>

			

			

		

			

			
			


		


	

	<div class="center">

		<%= link_to("Delete", delete_school_attachment_path(@school_attachment.id),:class=>"waves-effect waves-light btn red darken-4") %>
		
		<%= link_to("Edit", edit_school_attachment_path(@school_attachment.id),:class=>"waves-effect waves-light btn grey darken-4") %>
		
	</div>

	</div>
	
</div>
</div>

</div>

</div>










