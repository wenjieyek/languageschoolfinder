<%@page_title="Delete Timetable" %>


<%= render(:partial=>'layouts/school_navigation') %>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">


	<%=form_for(@timetable, :method => 'delete') do |f| %>
	<h4>Delete</h4>

	<p>Are you sure you want to permanently delete this time?</p>

	
	

	<div class="center">

		
		<%= f.submit("Delete",:class=>"waves-effect waves-light btn red darken-4") %>
		<%=link_to("Back",timetables_path,:class=>'bwaves-effect waves-light btn teal lighten-2')%>
	</div>




	<% end %>

		


	</div>

	
	
</div>
</div>

</div>

</div>


















	