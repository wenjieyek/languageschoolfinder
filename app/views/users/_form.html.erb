<%= error_messages_for(f.object) %>

<div class="row">
<div class="input-field col s12 m12">
    <%=f.label("Username")%>
    <%=f.text_field(:username)%>
</div>

</div>



<div class="row">
<div class="input-field col s12 m6">
    <%=f.label(:password)%>
    <%=f.password_field(:password)%>
</div>
<div class="input-field col s12 m6">
    <%=f.label(:password)%>
    <%=f.password_field(:password_confirmation)%>
</div>
</div>



<div class="row">
<div class="input-field col s12 m12">
    <%=f.label(:email)%>
    <%=f.text_field(:email)%>
</div>


</div>



<div class="row">
<div class="input-field col s12">
    <%=f.label("Address")%>
    <%=f.text_area(:address,:area=>'40x10',:class=>"materialize-textarea")%>

</div>
</div>

<div class="row">
<div class="file-field input-field s12" >
<div class="waves-effect waves-light btn teal lighten-2">
      <span>Upload Profile Picture</span>
      <%=f.file_field(:profilepicture)%>
</div>
<div class="file-path-wrapper">
      <input class="file-path validater" type="text" placeholder="Upload School's Picture" disabled>
</div>
</div>

</div>














