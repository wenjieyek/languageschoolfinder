<% @page_title="Inquiries List" %>

<%= render(:partial=>'layouts/school_navigation') %>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">
<h5>Inquiries List </h5>
<p><%=link_to("Add Course",new_inquiry_path) %></p>

	<table class="responsive-table">
		<thead>
			<tr>
			<td>Title</td>
			<td>Course</td>
			
			<td>Action</td>
			</tr>
		</thead>

		<tbody>
	
		<% @inquiries.each do |inquiries| %>
			<tr>
			<td><%= inquiries.title %></td>
			<td><%= inquiries.course %></td>
			
			<td><%= link_to("Show", inquiry_path(inquiries.id)) %></td>
			
			</tr>


		<% end %>


		</tbody>

	</table>


		


	</div>

	
	
</div>
</div>

</div>

</div>













