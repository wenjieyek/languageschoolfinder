<%@page_title="Delete Admin" %>



<%= render(:partial=>'layouts/admin_navigation') %>


<div class="col s12 m9 l9">
<!-- Teal page content  -->
<div class="card">
<div class="card-content">


	<%=form_for(@admin, :method => 'delete') do |f| %>
	<h4>Delete <%=f.object.username %> </h4>

	<p>Are you sure you want to permanently delete this admin?</p>

	
	

	
	<div class="center">

	<%= f.submit("Delete Admin",:class=>"waves-effect waves-light btn red darken-4") %>
		<%=link_to("Back",admins_path,:class=>'bwaves-effect waves-light btn grey darken-4')%>
		

	</div>
		
		
	</div>




	<% end %>

		


	</div>

	
	

</div>

</div>

</div>


















	